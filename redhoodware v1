local au=i:addTab"Settings"


local av=au:createGroup('left','Create Configs')

av:addTextbox{text="Name",flag="config_name"}
av:addButton{text="Load",callback=i.loadConfig}


local aw=au:createGroup('left','Config Settings')

aw:addConfigbox{flag='test',values={}}
aw:addButton{text="Load",callback=i.loadConfig}
aw:addButton{text="Update",callback=i.saveConfig}
aw:addButton{text="Delete",callback=i.deleteConfig}
aw:addButton{text="Refresh",callback=i.refreshConfigs}

local ax=Instance.new"Highlight"
ax.FillColor=Color3.fromRGB(160,160,160)
ax.OutlineColor=Color3.fromRGB(255,35,35)


al.RS1=game:GetService"RunService".Heartbeat:Connect(function()
r.pred=get_pred()
an.Position=Vector2.new(ak.X,ak.Y+ah:GetGuiInset().Y)
an.Radius=r.fov.size*3
an.Visible=r.fov.visible
if r.on and r.mode=="FOV"then
GetClosestPlayer()
end
if r.on and r.target then
r.cf=GetClosestPointOfPart(r.target.Character[r.part])
end
if r.on and r.hl and r.target then
if ax.Parent~=r.target.Character then
ax.Parent=r.target.Character
end
else
if ax.Parent~=game.CoreGui then
ax.Parent=game.CoreGui
end
end
if r.on and r.target and r.assist and r.mode=="Target"then
local ay=r.target.Character
local az=r.cf
local t=az+Vector3.new(ay.HumanoidRootPart.Velocity.X,(ay.HumanoidRootPart.Velocity.Y*0.3),ay.HumanoidRootPart.Velocity.Z)*r.pred
local u=CFrame.new(aj.CFrame.p,t)
aj.CFrame=aj.CFrame:Lerp(u,r.assistv,Enum.EasingStyle.Exponential,Enum.EasingDirection.InOut)
end
end)

game:GetService"UserInputService".InputBegan:Connect(function(ay,az)
if az then return end
if ay.KeyCode~=r.key then return end
if r.on and r.mode=="Target"then
if r.target~=nil then
r.target=nil
else
GetClosestPlayer()
end
end
end)

function Silent()
if r.on and r.target~=nil then
local ay=r.target.Character
local az=CFrame.new(r.cf)
local t
local u={
ay.HumanoidRootPart,
ay.Head,
ay.UpperTorso,
ay.LowerTorso,
ay.LeftFoot,
ay.RightFoot,
ay.LeftHand,
ay.RightHand
}


local v=u[math.random(1,#u)]


if v and v:IsA"BasePart"then
if v.Velocity.Y<-20 then
t=az.Position+Vector3.new(v.Velocity.X,(v.Velocity.Y*0.3),v.Velocity.Z)*r.pred
else
t=az.Position+v.Velocity*r.pred
end
end
MainEvent():FireServer("MOUSE",t)
end
end

game.Players.LocalPlayer.Character.ChildAdded:Connect(function(ay)
if ay:IsA"Tool"then
ay.Activated:Connect(function()
Silent()
end)
end
end)

game.Players.LocalPlayer.CharacterAdded:Connect(function(ay)
ay.ChildAdded:Connect(function(az)
if az:IsA"Tool"then
az.Activated:Connect(function()
Silent()
end)
end
end)
end)

do
ad:BindToRenderStep("ESPUpdate",100,function()
do
if p.Character then
for ay,az in next,l.ESPCache do
az:Update();
end;
end;
end;
end);
end
