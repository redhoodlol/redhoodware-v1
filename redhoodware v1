local function updateValue(al,am)
if typeof(al)=="table"then al=am end
i.flags[ab.flag]=al
Y.BackgroundColor3=al
if ab.callback then
ab.callback(al)
end
end

local al,am=Color3.new(1,1,1),Color3.new(0,0,0)
local an={Color3.new(1,0,0),Color3.new(1,1,0),Color3.new(0,1,0),Color3.new(0,1,1),Color3.new(0,0,1),Color3.new(1,0,1),Color3.new(1,0,0)}
local ao=game:GetService"RunService".Heartbeat

local ap,aq,ar=0,0,0
local as,at=0,0

af.MouseEnter:Connect(function()
local au=af.InputBegan:connect(function(au)
if au.UserInputType==Enum.UserInputType.MouseButton1 then
while ao:wait()and a:IsMouseButtonPressed(Enum.UserInputType.MouseButton1)do
i.colorpicking=true
local av=(ar-af.AbsolutePosition.Y-36)/af.AbsoluteSize.Y
local aw=math.max(1,math.min(7,math.floor(((av*7+0.5)*100))/100))
local ax=an[math.floor(aw)]
local ay=an[math.ceil(aw)]
local az=al:lerp(ai.BackgroundColor3,as):lerp(am,at)
ai.BackgroundColor3=ax:lerp(ay,aw-math.floor(aw))or Color3.new(0,0,0)
updateValue(az)
end
i.colorpicking=false
end
end)
local av
av=af.MouseLeave:connect(function()
au:disconnect()
av:disconnect()
end)
end)

ai.MouseEnter:Connect(function()
local au=ai.InputBegan:connect(function(au)
if au.UserInputType==Enum.UserInputType.MouseButton1 then
while ao:wait()and a:IsMouseButtonPressed(Enum.UserInputType.MouseButton1)do
i.colorpicking=true
local av=(ap-ai.AbsolutePosition.X)/ai.AbsoluteSize.X
local aw=(aq-ai.AbsolutePosition.Y-36)/ai.AbsoluteSize.Y
local ax=al:lerp(ai.BackgroundColor3,av):lerp(am,aw)
updateValue(ax)
as,at=av,aw
end
i.colorpicking=false
end
end)
local av
av=ai.MouseLeave:connect(function()
au:disconnect()
av:disconnect()
end)
end)

af.MouseMoved:connect(function(au,av)
ar=av
end)

ai.MouseMoved:connect(function(au,av)
ap,aq=au,av
end)

table.insert(i.toInvis,_)
i.flags[ab.flag]=Color3.new(1,1,1)
i.options[ab.flag]={type="colorpicker",changeState=updateValue,skipflag=ab.skipflag,oldargs=ab}

updateValue(ab.color or Color3.new(1,1,1))
end
return V
end
