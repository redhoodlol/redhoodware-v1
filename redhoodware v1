local function updateValue(as,at)
if typeof(as)=="table"then as=at end
i.flags[ab.flag]=as
ae.BackgroundColor3=as
if ab.callback then
ab.callback(as)
end
end

local as,at=Color3.new(1,1,1),Color3.new(0,0,0)
local au={Color3.new(1,0,0),Color3.new(1,1,0),Color3.new(0,1,0),Color3.new(0,1,1),Color3.new(0,0,1),Color3.new(1,0,1),Color3.new(1,0,0)}
local av=game:GetService"RunService".Heartbeat

local aw,ax,ay=0,0,0
local az,M=0,0

am.MouseEnter:Connect(function()
local N=am.InputBegan:connect(function(N)
if N.UserInputType==Enum.UserInputType.MouseButton1 then
while av:wait()and a:IsMouseButtonPressed(Enum.UserInputType.MouseButton1)do
i.colorpicking=true
local O=(ay-am.AbsolutePosition.Y-36)/am.AbsoluteSize.Y
local P=math.max(1,math.min(7,math.floor(((O*7+0.5)*100))/100))
local Q=au[math.floor(P)]
local R=au[math.ceil(P)]
local S=as:lerp(ap.BackgroundColor3,az):lerp(at,M)
ap.BackgroundColor3=Q:lerp(R,P-math.floor(P))or Color3.new(0,0,0)
updateValue(S)
end
i.colorpicking=false
end
end)
local O
O=am.MouseLeave:connect(function()
N:disconnect()
O:disconnect()
end)
end)

ap.MouseEnter:Connect(function()
local N=ap.InputBegan:connect(function(N)
if N.UserInputType==Enum.UserInputType.MouseButton1 then
while av:wait()and a:IsMouseButtonPressed(Enum.UserInputType.MouseButton1)do
i.colorpicking=true
local O=(aw-ap.AbsolutePosition.X)/ap.AbsoluteSize.X
local P=(ax-ap.AbsolutePosition.Y-36)/ap.AbsoluteSize.Y
local Q=as:lerp(ap.BackgroundColor3,O):lerp(at,P)
updateValue(Q)
az,M=O,P
end
i.colorpicking=false
end
end)
local O
O=ap.MouseLeave:connect(function()
N:disconnect()
O:disconnect()
end)
end)

am.MouseMoved:connect(function(N,O)
ay=O
end)

ap.MouseMoved:connect(function(N,O)
aw,ax=N,O
end)

table.insert(i.toInvis,ai)
i.flags[ab.flag]=Color3.new(1,1,1)
i.options[ab.flag]={type="colorpicker",changeState=updateValue,skipflag=ab.skipflag,oldargs=ab}

updateValue(ab.color or Color3.new(1,1,1))
end
