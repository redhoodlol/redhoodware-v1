function updateValue(ag)
if i.colorpicking then return end
i.flags[ab.flag]=ag
af.Text=q[ag]or ag.Name
end
a.InputBegan:Connect(function(ag)
local ah=ag.KeyCode==Enum.KeyCode.Unknown and ag.UserInputType or ag.KeyCode
if ac then
if not table.find(i.blacklisted,ah)then
ac=false
i.flags[ab.flag]=ah
af.Text=q[ah]or ah.Name
af.TextColor3=Color3.fromRGB(155,155,155)
end
end
if not ac and ah==i.flags[ab.flag]and ab.callback then
ab.callback()
end
end)

af.MouseButton1Click:Connect(function()
if i.colorpicking then return end
i.flags[ab.flag]=Enum.KeyCode.Unknown
af.Text="..."
af.TextColor3=Color3.new(0.2,0.2,0.2)
ac=true
end)

i.flags[ab.flag]=Enum.KeyCode.Unknown
i.options[ab.flag]={type="keybind",changeState=updateValue,skipflag=ab.skipflag,oldargs=ab}

updateValue(ab.key or Enum.KeyCode.Unknown)
end
return L,E
end
return x
end

function contains(aa,ab)
for ac,ad in pairs(aa)do
if ad==ab then return true end
end
return false
end

function i.createConfig(aa)
local ab=i.flags.config_name
if contains(i.options.selected_config.values,ab)then return i:notify(ab..".cfg already exists!")end
if ab==""then return i:notify"Put a name goofy"end
local ac={}
for ad,ae in next,i.flags do
if i.options[ad].skipflag then continue end
if typeof(ae)=="Color3"then
ac[ad]={ae.R,ae.G,ae.B}
elseif typeof(ae)=="EnumItem"then
ac[ad]={string.split(tostring(ae),".")[2],string.split(tostring(ae),".")[3]}
else
ac[ad]=ae
end
end
writefile("OsirisCFGS/"..ab..".cfg",game:GetService"HttpService":JSONEncode(ac))
i:notify("Succesfully created config "..ab..".cfg!")
i:refreshConfigs()
end

function i.saveConfig(aa)
local ab=i.flags.selected_config
local ac={}
for ad,ae in next,i.flags do
if i.options[ad].skipflag then continue end
if typeof(ae)=="Color3"then
ac[ad]={ae.R,ae.G,ae.B}
elseif typeof(ae)=="EnumItem"then
ac[ad]={string.split(tostring(ae),".")[2],string.split(tostring(ae),".")[3]}
else
ac[ad]=ae
end
end
writefile("OsirisCFGS/"..ab..".cfg",game:GetService"HttpService":JSONEncode(ac))
i:notify("Succesfully updated config "..ab..".cfg!")
i:refreshConfigs()
end

function i.loadConfig(aa)
local ab=i.flags.selected_config
if not isfile("OsirisCFGS/"..ab..".cfg")then
i:notify"Config file not found!"
return
end
local ac=game:GetService"HttpService":JSONDecode(readfile("OsirisCFGS/"..ab..".cfg"))
for ad,ae in next,i.options do
spawn(function()pcall(function()
if ac[ad]then
if ae.type=="colorpicker"then
ae.changeState(Color3.new(ac[ad][1],ac[ad][2],ac[ad][3]))
elseif ae.type=="keybind"then
ae.changeState(Enum[ac[ad][1] ][ac[ad][2] ])
else
if ac[ad]~=i.flags[ad]then
ae.changeState(ac[ad])
end
end
else
if ae.type=="toggle"then
ae.changeState(false)
elseif ae.type=="slider"then
ae.changeState(ae.args.value or 0)
elseif ae.type=="textbox"or ae.type=="list"or ae.type=="cfg"then
ae.changeState(ae.args.value or ae.args.text or"")
elseif ae.type=="colorpicker"then
ae.changeState(ae.args.color or Color3.new(1,1,1))
elseif option.type=="list"then
ae.changeState""
elseif option.type=="keybind"then
ae.changeState(ae.args.key or Enum.KeyCode.Unknown)
end
end
end)end)
end
i:notify("Succesfully loaded config "..ab..".cfg!")
end

function i.refreshConfigs(aa)
local ab={}
for ac,ad in next,listfiles"OsirisCFGS"do
table.insert(ab,ad)
end
i.options.selected_config.refresh(ab)
end

function i.deleteConfig(aa)
if isfile("OsirisCFGS/"..i.flags.selected_config..".cfg")then
delfile("OsirisCFGS/"..i.flags.selected_config..".cfg")
i:refreshConfigs()
end
end


