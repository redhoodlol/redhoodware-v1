local function updateValue(an)
if an==nil then ah.Text="nil"return end
if ab.multiselect then
if type(an)=="string"then
if not table.find(i.options[ab.flag].values,an)then return end
if table.find(i.flags[ab.flag],an)then
for ao,ap in pairs(i.flags[ab.flag])do
if ap==an then
table.remove(i.flags[ab.flag],ao)
end
end
else
table.insert(i.flags[ab.flag],an)
end
else
i.flags[ab.flag]=an
end
local ao=""
for ap,aq in pairs(i.flags[ab.flag])do
local ar=ap~=#i.flags[ab.flag]and","or""
ao=ao..aq..ar
end
if ao==""then ao="..."end
for ap,aq in next,al:GetChildren()do
if aq.ClassName~="Frame"then continue end
aq.off.TextColor3=Color3.new(0.65,0.65,0.65)
for ar,as in next,i.flags[ab.flag]do
if aq.Name==as then
aq.off.TextColor3=Color3.new(1,1,1)
end
end
end
ah.Text=ao
if ab.callback then
ab.callback(i.flags[ab.flag])
end
else
if not table.find(i.options[ab.flag].values,an)then an=i.options[ab.flag].values[1]end
i.flags[ab.flag]=an
for ao,ap in next,al:GetChildren()do
if ap.ClassName~="Frame"then continue end
ap.off.TextColor3=Color3.new(0.65,0.65,0.65)
if ap.Name==i.flags[ab.flag]then
ap.off.TextColor3=Color3.new(1,1,1)
end
end
ak.Visible=false
if i.flags[ab.flag]then
ah.Text=i.flags[ab.flag]
if ab.callback then
ab.callback(i.flags[ab.flag])
end
end
end
end

function refresh(an)
for ao,ap in next,al:GetChildren()do
if ap.ClassName=="Frame"then
ap:Destroy()
end
ak.Size=UDim2.new(0,203,0,0)
end
for ao,ap in pairs(an)do
ak.Size+=UDim2.new(0,0,0,20)

local aq=Instance.new"Frame"
local ar=Instance.new"TextButton"
local as=Instance.new"TextLabel"

aq.Name=ap
aq.Parent=al
aq.BackgroundColor3=Color3.fromRGB(255,255,255)
aq.BackgroundTransparency=1.000
aq.Size=UDim2.new(1,0,0,20)

ar.Name="button"
ar.Parent=aq
ar.BackgroundColor3=Color3.fromRGB(35,35,35)
ar.BackgroundTransparency=0.850
ar.BorderSizePixel=0
ar.Size=UDim2.new(1,0,1,0)
ar.Font=Enum.Font.SourceSans
ar.Text=""
ar.TextColor3=Color3.fromRGB(0,0,0)
ar.TextSize=14.000

as.Name="off"
as.Parent=aq
as.BackgroundColor3=Color3.fromRGB(255,255,255)
as.BackgroundTransparency=1.000
as.Position=UDim2.new(0,4,0,0)
as.Size=UDim2.new(0,0,1,0)
as.Font=Enum.Font.Code
as.Text=ap
as.TextColor3=ab.multiselect and Color3.new(0.65,0.65,0.65)or Color3.new(1,1,1)
as.TextSize=14.000
as.TextStrokeTransparency=0.000
as.TextXAlignment=Enum.TextXAlignment.Left

ar.MouseButton1Click:Connect(function()
updateValue(ap)
end)
end
i.options[ab.flag].values=an
updateValue(table.find(i.options[ab.flag].values,i.flags[ab.flag])and i.flags[ab.flag]or i.options[ab.flag].values[1])
end

af.MouseButton1Click:Connect(function()
if not i.colorpicking then
ak.Visible=not ak.Visible
end
end)
af.MouseEnter:connect(function()
ae.BorderColor3=i.libColor
end)
af.MouseLeave:connect(function()
ae.BorderColor3=Color3.fromRGB(60,60,60)
end)

table.insert(i.toInvis,ak)
i.flags[ab.flag]=ab.multiselect and{}or""
i.options[ab.flag]={type="list",changeState=updateValue,values=ab.values,refresh=refresh,skipflag=ab.skipflag,oldargs=ab}

refresh(ab.values)
updateValue(ab.value or not ab.multiselect and ab.values[1]or"abcdefghijklmnopqrstuwvxyz")
end
