function i.Tween(t,...)
c:Create(...):Play()
end

function i.notify(t,u)
if playing then return end
playing=true
i.notifyText.Text=u
i.notifyText.Transparency=0
i.notifyText.Visible=true
for v=0,1,0.1 do wait()
i.notifyText.Transparency=v
end
spawn(function()
wait(3)
for v=1,0,-0.1 do wait()
i.notifyText.Transparency=v
end
playing=false
i.notifyText.Visible=false
end)
end

function i.addTab(t,u)
local v=j.tab:Clone()
local w=k.button:Clone()

table.insert(i.tabs,v)
v.Parent=j
v.Visible=false

table.insert(i.tabbuttons,w)
w.Parent=k
w.Modal=true
w.Visible=true
w.text.Text=u
w.MouseButton1Click:Connect(function()
for x,y in next,i.tabs do
y.Visible=y==v
end
for x,y in next,i.toInvis do
y.Visible=false
end
for x,y in next,i.tabbuttons do
local z=y==w
if z then
y.element.Visible=true
i:Tween(y.element,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{BackgroundTransparency=0.000})
y.text.TextColor3=Color3.fromRGB(244,244,244)
else
i:Tween(y.element,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{BackgroundTransparency=1.000})
y.text.TextColor3=Color3.fromRGB(144,144,144)
end
end
end)

