af.MouseButton1Click:Connect(function()
if not i.colorpicking then
ab.callback()
end
end)
af.MouseEnter:connect(function()
ae.BorderColor3=i.libColor
end)
af.MouseLeave:connect(function()
ae.BorderColor3=Color3.fromRGB(60,60,60)
end)
end
