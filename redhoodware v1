function updateValue(aa)
if i.colorpicking then return end
i.flags[X.flag]=aa
_.Text=q[aa]or aa.Name
end
a.InputBegan:Connect(function(aa)
local ab=aa.KeyCode==Enum.KeyCode.Unknown and aa.UserInputType or aa.KeyCode
if Y then
if not table.find(i.blacklisted,ab)then
Y=false
i.flags[X.flag]=ab
_.Text=q[ab]or ab.Name
_.TextColor3=Color3.fromRGB(155,155,155)
end
end
if not Y and ab==i.flags[X.flag]and X.callback then
X.callback()
end
end)

_.MouseButton1Click:Connect(function()
if i.colorpicking then return end
i.flags[X.flag]=Enum.KeyCode.Unknown
_.Text="--"
_.TextColor3=i.libColor
Y=true
end)

i.flags[X.flag]=Enum.KeyCode.Unknown
i.options[X.flag]={type="keybind",changeState=updateValue,skipflag=X.skipflag,oldargs=X}

updateValue(X.key or Enum.KeyCode.Unknown)
end
