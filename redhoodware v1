function get_pred()
local ao=game:GetService"Stats".Network.ServerStatsItem["Data Ping"]:GetValueString()
local ap=tostring(ao)
local aq=ap:split" "
local ar=tonumber(aq[1])
return tonumber(ar/225*0.1+0.1)
end

CheckDistance=function(ao)
if(ao.Character:FindFirstChild"HumanoidRootPart".Position-game.Players.LocalPlayer.Character:FindFirstChild"HumanoidRootPart".Position).Magnitude>r.dist then
return true
else
return false
end
end

OnScreen=function(ao)local
ap, aq=aj:WorldToScreenPoint(ao.Position)
return aq
end

WTS=function(ao)
local ap=aj:WorldToScreenPoint(ao.Position)
return v2.new(ap.X,ap.Y)
end


FilterObjs=function(ao)
if string.find(ao.Name,"Gun")then
return
end
if table.find({"Part","MeshPart","BasePart"},ao.ClassName)then
return true
end
end


RayCastCheck=function(ao,ap)
local aq=ai.Character or ai.CharacterAdded.Wait(ai.CharacterAdded)
local ar=aj.CFrame.Position

local as=RaycastParams.new()
as.FilterType=Enum.RaycastFilterType.Blacklist
as.FilterDescendantsInstances={aq,aj}

local at=ag.Raycast(ag,ar,ao.Position-ar,as)

if(at)then
local au=at.Instance
local av=(not au or Instance.new"Part".IsDescendantOf(au,ap))

return av
end
return false
end

GetMagnitudeFromMouse=function(ao)
local ap,aq=aj:WorldToScreenPoint(ao.Position)
if aq then
local ar=(Vector2.new(ap.X,ap.Y)-Vector2.new(ak.X,ak.Y)).Magnitude
return ar
end
return math.huge
end

GetClosestPlayer=function()
local ao
local ap=math.huge
for aq,ar in pairs(ac:GetPlayers())do
if ar.Character and ar~=ai and ar.Character:FindFirstChild"HumanoidRootPart"then
if not OnScreen(ar.Character.HumanoidRootPart)then
continue
end
if r.visible and not RayCastCheck(ar.Character.HumanoidRootPart,ar.Character)then
continue
end
if r.friend and game.Players.LocalPlayer:IsFriendsWith(ar.UserId)then
continue
end
if r.distance and CheckDistance(ar)then
continue
end
local as=GetMagnitudeFromMouse(ar.Character.HumanoidRootPart)
if(as<ap and r.fov.size*3+as*0.3>as)then
ap=as
ao=ar
end
end
end
r.target=ao
end

local function MainEvent()
for ao,ap in pairs(game.ReplicatedStorage:GetChildren())do
if ap.Name=="MainEvent"or ap.Name=="Bullets"or ap.Name==".gg/untitledhood"or ap.Name=="Remote"or ap.Name=="MAINEVENT"then
return ap
end
end
end

GetClosestBodyPart=function()
local ao=r.target.Character
local ap=1/0
local aq
if(ao and ao:GetChildren())then
for ar,as in next,ao:GetChildren()do
if FilterObjs(as)and OnScreen(as)then
local at=(WTS(as)-Vector2.new(ak.X,ak.Y)).Magnitude
if(at<ap)then
ap=at
aq=as
end
end
end
end
r.part=tostring(aq)
end


function GetClosestPointOfPart(ao)
local ap=ak.UnitRay
ap=ap.Origin+(ap.Direction*(ao.Position-ap.Origin).Magnitude)
local aq=
(ap.Y>=(ao.Position-ao.Size/2).Y and ap.Y<=(ao.Position+ao.Size/2).Y)and
(ao.Position+Vector3.new(0,-ao.Position.Y+ap.Y,0))or ao.Position
local ar=RaycastParams.new()
ar.FilterType=Enum.RaycastFilterType.Whitelist
ar.FilterDescendantsInstances={ao}
local as=game:GetService"Workspace":Raycast(ap,(aq-ap),ar)
if as then
return as.Position
else
return ak.Hit.Position
end
end

do

m.WDown=false;
m.ADown=false;
m.SDown=false;
m.DDown=false;

function m.Tween(ao,...)
local ap=game:GetService"TweenService":Create(...)
ap:Play();
return ap
end;


function m.FindPlayer(ao,ap)
ap=ap:lower();
local aq={};
for ar,as in next,ac:GetPlayers()do
if as and as~=p then
local at=as.Name:lower();
local au=as.DisplayName:lower();
if at==ap then
table.insert(aq,as);
break
end;

if string.sub(at,1,#ap)==ap or string.sub(au,1,#ap)==ap then
table.insert(aq,as);
end;
end;
end

return aq
end;

function m.FireAimlock(ao)
local ap=p.Character:FindFirstChildOfClass"Tool";
if ap and ap:FindFirstChild"Shoot"then
local aq=m.AimlockTarget.Character;
local ar=aq:FindFirstChild(am.AimPart);
local as;

if ar then
local at=ar.Velocity;
if am.YPrediction==false then
at=Vector3.new(at.X,0,at.Z);
end;

if am.AimlockMode=="default"or am.AimlockMode=="velocity"then
as=ar.CFrame+at/am.AimVelocity+at/math.huge
end;

if am.AimlockMode=="movedirection"then
at=ar:GetAttribute"CalculatedVelocity";
if not at then
at=Vector.new(0,0,0);
end;

if am.YPrediction==false then
at=Vector3.new(at.X,0,at.Z);
end;

local au=aq:FindFirstChildOfClass"Humanoid";
local av=au.MoveDirection;

if av.Magnitude~=0 then
as=ar.CFrame+au.MoveDirection*(at.Unit+Vector3.new(Ping/1000,Ping/1000,Ping/1000))
else
as=ar.CFrame;
end;
end;

ap.Shoot:FireServer(as);
end;
end;
end;

function m.Fly(ao)
local ap=p.Character:FindFirstChild"Torso";

local aq,ar=Instance.new("BodyVelocity",ap),Instance.new("BodyGyro",ap);

aq.Velocity=Vector3.new(0,0.1,0);
aq.MaxForce=Vector3.new(9e9,9e9,9e9);

ar.CFrame=ap.CFrame;
ar.MaxTorque=Vector3.new(9e9,9e9,9e9);
ar.P=9e4;
m.BodyGyro=ar;
m.BodyVelocity=aq;
task.spawn(function()
repeat wait()until m.Flying~=true
ar:Destroy();
aq:Destroy();
end);
end;

function m.ApplyHitmarker(ao,ap)
local aq=ap.Character;

if aq then
local ar=aq:WaitForChild"Humanoid";
ar.ChildAdded:Connect(function(as)
task.wait(0.1);
if as.Name=="creator"then
if tostring(as.Value)==p.Name and am.Hitmarkers and ap.Character:FindFirstChild"Head"then
m.Hitmarker:Play();
l:NewHitmarker(ap.Character:FindFirstChild"Head".Position);
math.floor((o.CFrame.p-ap.Character:FindFirstChild"HumanoidRootPart".CFrame.p).Magnitude);

end;
end;
end);
end;

ap.CharacterAdded:Connect(function()
task.spawn(function()
repeat aq=ap.Character until aq~=nil;
local ar=aq:WaitForChild"Humanoid";
ar.ChildAdded:Connect(function(as)
task.wait(0.1);
if as.Name=="creator"then
if tostring(as.Value)==p.Name and am.Hitmarkers then
m.Hitmarker:Play();
l:NewHitmarker(ap.Character:FindFirstChild"Head".Position);
math.floor((o.CFrame.p-ap.Character:FindFirstChild"HumanoidRootPart".CFrame.p).Magnitude);

end;
end;
end);
end);
end);
end;

function m.CharacterAdded(ao)
repeat task.wait()until p.Character;
local ap=p.Character:WaitForChild"Humanoid";
repeat wait()until ap~=nil;

if m.Flying then
m:Fly();
end;

if am.DeathTP and HasInit then
p.Character:WaitForChild"HumanoidRootPart".CFrame=LastDeathPosition;
end;

do
p.Character.DescendantAdded:Connect(function(aq)
if aq.Name=="Bone"then
if am.QuickRespawn then
p.Character.Humanoid.Health=0;
p.Character.Humanoid:ChangeState(15);
end;
LastDeathPosition=p.Character:WaitForChild"HumanoidRootPart".CFrame;
end;

if aq.Name=="creator"then
if am.AttackerWarnings then
local ar=ac:FindFirstChild(tostring(aq.Value));
local as=ar.Character;
local at=as:FindFirstChildOfClass"Tool"and as:FindFirstChildOfClass"Tool".Name or"None";
if at~="None"then



end;
end;
end;
end);

ap:GetPropertyChangedSignal"Health":Connect(function()
if ap.Health==0 then
if am.QuickRespawn then
p.Character.Humanoid:ChangeState(15);
end;
LastDeathPosition=p.Character:WaitForChild"HumanoidRootPart".CFrame;
end;
end);

local aq=p.Character:WaitForChild"Stam";
aq:GetPropertyChangedSignal"Value":Connect(function()
if am.InfiniteStamina then
aq.Value=100;
end;
end);
end;

end;
end;

do
n.__index=n;
function n.GetCharacter(ao)
local ap=ao.Player.Character;

return ap
end;

function n.CheckGamepass(ao,ap)
return MarketplaceService:UserOwnsGamePassAsync(ao.Player.UserId,ap)
end;

function n.GetRoot(ao)
local ap=ao:GetCharacter();
if ap then
if ap:FindFirstChild"HumanoidRootPart"then
return ap:FindFirstChild"HumanoidRootPart"
end

if ap:FindFirstChild"Torso"then
return ap:FindFirstChild"Torso"
end;

return ap.PrimaryPart
end;
return nil
end;

function n.GetDistance(ao)
local ap=ao:GetCharacter();
if ap then
local aq=ao:GetRoot();

if aq then
return math.floor((o.CFrame.p-aq.CFrame.p).Magnitude)
end;
end;
return 0
end;

function n.GetName(ao)
return ao.Player.Name
end;

function n.GetWeapon(ao)
local ap=ao:GetCharacter();
if ap then
local aq=ap:FindFirstChildOfClass"Tool";
if aq then
return aq,aq.Name
end;
end;

return nil,"None"
end;

function n.GetHealth(ao)
local ap=ao:GetCharacter();
if ap and ap:FindFirstChildOfClass"Humanoid"then
return ap:FindFirstChildOfClass"Humanoid".Health,ap:FindFirstChildOfClass"Humanoid".MaxHealth
end;
return 0,100
end;

function n.GetAmmo(ao)
local ap=ao:GetCharacter();
if ap and ao:GetWeapon()then
local aq,ar=ao:GetWeapon();

if table.find(ao.GunNames,ar)then
local as=aq:FindFirstChild"Ammo";
local at=aq:FindFirstChild"Clips";

if as and at then
return as.Value,at.Value
end;
end;
end;

return 0,0
end;

function n.GetBoundingBox(ao)
local ap=ao:GetRoot();

local aq={};
local ar,as=o:WorldToViewportPoint(ap.Position);

local at=1/(ar.Z*math.tan(math.rad(o.FieldOfView/2))*2)*1000;
local au=4*at;
local av=7*at;

aq.Width=au;
aq.Height=av;

local aw,ax=l:FloorVector(Vector2.new(math.max(au,7),math.max(av,12))),l:FloorVector(Vector2.new(ar.X-au/2,(ar.Y-1)-av/2));
local ay=Vector2.new(ax.X+aw.X/2,ax.Y+aw.Y/2);

aq.BoxSize=aw;
aq.BoxPos=ax;
aq.Center=ay;
aq.IsOnScreen=as;

return aq
end;

function n.SetInvisible(ao)
local ap=ao.Components;

if ap.Name then
ap.Name.Visible=false;
ap.Reloading.Visible=false;
ap.Weapon.Visible=false;
ap.Ammo.Visible=false;
ap.Distance.Visible=false;
ap.Health.Visible=false;
ap.Vest.Visible=false;

ap.Box.Visible=false;
ap.BoxOutline.Visible=false;

ap.HealthbarOutline.Visible=false;
ap.Healthbar.Visible=false;
end;
end;

function n.CheckVest(ao)
if ao:GetCharacter():FindFirstChild"BulletResist"then
return true
end;
return false
end;

function n.Update(ao)
local ap=ao:GetCharacter()
local aq=ao.Components;local ar=
ao.Player;
local as=ao.Highlight;


local at=aq.Box;
local au=aq.BoxOutline;


local av=aq.Name;
local aw=aq.Distance;
local ax=aq.Health;
local ay=aq.Reloading;
local az=aq.Weapon;
local t=aq.Ammo;
local u=aq.Vest;

local v=aq.Healthbar;
local w=aq.HealthbarOutline;


if ap and ap:FindFirstChildOfClass"Humanoid"and ao:GetRoot()then

local x=ao:GetName();
local y,z=ao:GetAmmo();
local A=ao:GetDistance();
local B=ao:GetRoot();
local C=ao:GetBoundingBox();
local D=ao:CheckVest();
local E,F=ao:GetWeapon();
local G,H=ao:GetHealth();local

I, J, K, L=C.BoxSize, C.BoxPos, C.BoxCenter, C.IsOnScreen;


local M=l:FloorVector(Vector2.new(I.X/2+J.X,J.Y-16));
local N=l:FloorVector(Vector2.new(I.X/2+J.X,I.Y+J.Y+1));
local O=Vector2.new(J.X+I.X+8,J.Y-1)
local P=Vector2.new(J.X-28,(J.Y+I.Y)-1*I.Y);


local Q=0
local R=0
local S=0
local T=0

if B and L and at then
if am.ESPBox then
au.Visible=true;
au.Position=J;
au.Size=I;

at.Color=am.BoxColor;
at.Visible=true;
at.Size=I;
at.Position=J;
else
at.Visible=false;
au.Visible=false;
end;

do

if am.ESPName then
av.Visible=true;
av.Text=x;
av.Position=M+Vector2.new(0,Q);
av.Color=am.TextColor;
Q=Q-14;
else
av.Visible=false;
end;

if am.ESPDistance then
aw.Visible=true;
aw.Text=tostring(A).." studs";
aw.Position=N+Vector2.new(0,S);
aw.Color=am.TextColor
S=S+14;
else
aw.Visible=false;
end;

if am.ESPWeapon then
if az then
az.Visible=true;
az.Text=F;
az.Position=N+Vector2.new(0,S);
az.Color=am.TextColor;

S=S+14;
else
az.Visible=false;
end;
else
az.Visible=false;
end;

if am.ESPAmmo then
t.Position=N+Vector2.new(0,S);
t.Text="Ammo: "..tostring(y).." | Clips: "..tostring(z);
t.Visible=true;
t.Color=am.TextColor;
S=S+14;
else
t.Visible=false;
end;

if am.ESPVest then
u.Position=O+Vector2.new(0,T);
u.Center=false;

if D then
u.Visible=true;
u.Text="VEST";
u.Color=am.VestColor;
T=T+14;
else
u.Visible=false;
end;
else
u.Visible=false;
end;

if am.ESPReloading then
ay.Position=O+Vector2.new(0,T);
ay.Center=false;
if E and E:FindFirstChild"Reloader"then
if E.Reloader.Value then
ay.Text="Reloading";
ay.Color=Color3.fromRGB(100,95,192)
ay.Visible=true;
else
ay.Visible=false;
end;
else
ay.Visible=false;
end;

T=T+14;
else
ay.Visible=false;
end;

if am.ESPHealth then
ax.Text=tostring(math.floor(G));
ax.Position=P+Vector2.new(0,R);
ax.Visible=true;

R=R+14;
else
ax.Visible=false;
end;
end;

if am.ESPHealthbar then
local U=Vector2.new(J.X-5,J.Y+I.Y);
local V=Vector2.new(U.X,U.Y-(G/H)*I.Y);

v.Color=am.HealthLower:lerp(am.HealthHigher,G/H);

v.Visible=true;
w.Visible=true;

v.From=U;
v.To=V;

w.From=Vector2.new(U.X,J.Y+I.Y)+Vector2.new(0,1);
w.To=Vector2.new(U.X,U.Y-1*I.Y)+Vector2.new(0,-1);
else
v.Visible=false;
w.Visible=false;
end;

if am.ESPHighlight then
as.FillTransparency=am.HighlightFillTransparency;
as.OutlineTransparency=am.HighlightOutlineTransparency;
as.OutlineColor=am.HighlightOutlineColor;
as.FillColor=am.HighlightFillColor;
as.Adornee=ap;
as.Enabled=true;
else
as.Enabled=false;
end;
else

ao:SetInvisible()
end;
else

ao:SetInvisible()
end;
end;
end;

do
l.ESPCache={};
l.Hitmarkers={};
function l.FloorVector(ao,ap)
if typeof(ap)=="Vector2"then
return Vector2.new(math.floor(ap.X),math.floor(ap.Y))
else
return Vector3.new(math.floor(ap.X),math.floor(ap.Y),math.floor(ap.Z))
end;
end;

function l.Draw(ao,ap,aq)
local ar=Drawing.new(ap);
task.spawn(function()
for as,at in next,aq do
pcall(function()
ar[as]=at;
end)
end;
end);
return ar
end;

function l.NewLabel(ao)
local ap=l:Draw("Text",{
Text="",
Font=2,
Size=13,
Outline=true,
Center=true,
Color=Color3.new(1,1,1);
});
return ap
end;

function l.NewBox(ao)
local ap=l:Draw("Square",{
Color=Color3.new(0,0,0),
Filled=false;
Thickness=3,
});
local aq=l:Draw("Square",{
Color=Color3.new(1,1,1),
Filled=false;
Thickness=1,
});

return aq,ap
end;

function l.New(ao,ap)
if not l.ESPCache[ap]then
local aq={};
local ar=Instance.new("Highlight",af);

task.spawn(function()
do
ar.FillTransparency=1;
ar.OutlineTransparency=0;
ar.OutlineColor=Color3.fromRGB(255,255,255);
ar.DepthMode="AlwaysOnTop";
end;

do
local as,at=l:NewBox();
aq.Box=as;
aq.BoxOutline=at;
end;

do
aq.Name=l:NewLabel();
aq.Distance=l:NewLabel();
aq.Health=l:NewLabel();
aq.Weapon=l:NewLabel();
aq.Ammo=l:NewLabel();
aq.Reloading=l:NewLabel();
aq.Vest=l:NewLabel();
end;

do
aq.HealthbarOutline=l:Draw("Line",{
Thickness=3,
Color=Color3.new(0,0,0),
});

aq.Healthbar=l:Draw("Line",{
Thickness=1,
Color=Color3.new(0,1,0),
});
end;
end);
local as=setmetatable({
Components=aq,
Player=ap,
GunNames={"Uzi","Shotty","Glock"},
Highlight=ar,
},n)

l.ESPCache[ap]=as;
return as
end;
end;

function l.Remove(ao,ap)
if l.ESPCache[ap]then
for aq,ar in next,l.ESPCache[ap].Components do
ar:Remove();
end;

l.ESPCache[ap].Highlight:Destroy();
l.ESPCache[ap]=nil;
end;
end;

function l.NewHitmarker(ao,ap)
local aq=#l.Hitmarkers+1;
l.Hitmarkers[aq]={
Time=tick();
Position=ap;
Drawings={
[1]=l:Draw("Line",{
Thickness=1.5,
ZIndex=9000,
Color=am.HitmarkerColor,
}),
[2]=l:Draw("Line",{
Thickness=1.5,
ZIndex=9000,
Color=am.HitmarkerColor,
}),
[3]=l:Draw("Line",{
Thickness=1.5,
ZIndex=9000,
Color=am.HitmarkerColor,
}),
[4]=l:Draw("Line",{
Thickness=1.5,
ZIndex=9000,
Color=am.HitmarkerColor,
}),
};
};

task.spawn(function()
task.wait(1);
for ar=1,0,-0.1 do
task.wait(0.05);
l.Hitmarkers[aq].Drawings[1].Transparency=ar;
l.Hitmarkers[aq].Drawings[2].Transparency=ar;
l.Hitmarkers[aq].Drawings[3].Transparency=ar;
l.Hitmarkers[aq].Drawings[4].Transparency=ar;
end;
l.Hitmarkers[aq].Drawings[1]:Remove();
l.Hitmarkers[aq].Drawings[2]:Remove();
l.Hitmarkers[aq].Drawings[3]:Remove();
l.Hitmarkers[aq].Drawings[4]:Remove();
l.Hitmarkers[aq]=nil;
end);
end;

l.WeaponInfo=l:NewLabel();
l.PredCircle=l:Draw("Circle",{
Thickness=5,
Size=Vector2.new(10,10),
Filled=false;
Color=Color3.new(1,1,1),
ZIndex=9000,
});
end;


local ao=i:addTab"Legit"


local ap=ao:createGroup('left','Aiming')


ap:addList{text="Mode",flag="SilentMode",values={"FOV","Target"},callback=function(aq)
r.mode=aq
end}

ap:addList{text="Target Bind",flag="TargetBind",values=s.keys,callback=function(aq)
r.key=Enum.KeyCode[aq]
end}

ap:addToggle{text="Enabled",flag="Enabled",default=false,callback=function(aq)
r.on=aq
end}

ap:addToggle{text="Assist",flag="Assist",default=false,callback=function(aq)
r.assist=aq
end}

ap:addSlider{text="Assist Strength",flag="AssistStrength",min=1,max=100,default=58,callback=function(aq)
r.assistv=aq*0.001
end}

ap:addToggle{text="Highlight",flag="Highlight",default=false,callback=function(aq)
r.hl=aq
end}


local aq=ao:createGroup('right','Field of View')

aq:addToggle{text="Visible",flag="FovVisible",default=false,callback=function(ar)
r.fov.visible=ar
end}

aq:addSlider{text="Size",flag="FovSize",min=1,max=100,default=15,callback=function(ar)
r.fov.size=ar
end}


