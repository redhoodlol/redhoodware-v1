aj.TextSize=13.000
aj.TextStrokeTransparency=0.000
aj.TextXAlignment=Enum.TextXAlignment.Left

local ak=false
local al=false


local function updateValue(am)
if i.colorpicking then return end
if am~=0 then
af:TweenSize(UDim2.new(am/ab.max,0,1,0),Enum.EasingDirection.In,Enum.EasingStyle.Sine,0.01)
else
af:TweenSize(UDim2.new(0,1,1,0),Enum.EasingDirection.In,Enum.EasingStyle.Sine,0.01)
end
ah.Text=tostring(am)
i.flags[ab.flag]=am
if ab.callback then
ab.callback(am)
end
end
local function updateScroll()
if al or i.scrolling or not v.Visible or i.colorpicking then return end
while a:IsMouseButtonPressed(Enum.UserInputType.MouseButton1)and h.Enabled do b.RenderStepped:Wait()
i.scrolling=true
ah.TextColor3=Color3.fromRGB(255,255,255)
al=true
local am=ab.min+((f.X-ag.AbsolutePosition.X)/ag.AbsoluteSize.X)*(ab.max-ab.min)
if am<0 then am=0 end
if am>ab.max then am=ab.max end
if am<ab.min then am=ab.min end
updateValue(math.floor(am))
end
if al and not ak then
ah.TextColor3=Color3.fromRGB(255,255,255)
end
if not h.Enabled then
ak=false
end
al=false
i.scrolling=false
end
ag.MouseEnter:connect(function()
if i.colorpicking then return end
if al or ak then return end
ak=true
ae.BorderColor3=i.libColor
while ak do wait()
updateScroll()
end
end)
ag.MouseLeave:connect(function()
ak=false
ae.BorderColor3=Color3.fromRGB(60,60,60)
end)
if ab.value then
updateValue(ab.value)
end
i.flags[ab.flag]=0
i.options[ab.flag]={type="slider",changeState=updateValue,skipflag=ab.skipflag,oldargs=ab}
updateValue(ab.value or 0)
end
