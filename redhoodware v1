for ak,al in next,af:GetChildren()do
if al.ClassName~="Frame"then continue end
if al.text.Text==i.flags[ab.flag]then
al.text.TextColor3=i.libColor
else
al.text.TextColor3=Color3.fromRGB(255,255,255)
end
end
if i.flags[ab.flag]then
if ab.callback then
ab.callback(i.flags[ab.flag])
end
end
af.Visible=true
end
af:GetPropertyChangedSignal"CanvasPosition":Connect(function()
ai.Visible=(af.CanvasPosition.Y>1)
ah.Visible=(af.CanvasPosition.Y+1<(af.AbsoluteCanvasSize.Y-af.AbsoluteSize.Y))
end)


